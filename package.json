{
  "name": "llm-playground",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "migrations:generate": "drizzle-kit generate:pg",
    "migrations:push": "node -r esbuild-register src/lib/db/migrate.ts",
    "migrations:drop": "drizzle-kit drop --config=drizzle.config.ts",
    "seed": "node -r esbuild-register src/lib/db/seed.ts"
  },
  "dependencies": {
    "@clerk/nextjs": "^4.20.0",
    "@types/node": "20.2.6",
    "@types/react": "18.2.10",
    "@types/react-dom": "18.2.4",
    "@vercel/postgres": "^0.3.0",
    "autoprefixer": "10.4.14",
    "dotenv": "^16.1.4",
    "drizzle-orm": "^0.26.5",
    "eslint": "8.42.0",
    "eslint-config-next": "13.4.4",
    "next": "13.4.4",
    "postcss": "8.4.24",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "svix": "^1.4.12",
    "tailwindcss": "3.3.2",
    "typescript": "5.1.3"
  },
  "devDependencies": {
    "drizzle-kit": "^0.18.1",
    "esbuild-register": "^3.4.2"
  }
}
